<body>
  <main>
    <h1>Browser</h1>
    {{!-- maybe do the search if bored hahah --}}
    <form action="" method="">
      <input type="text" class="search" name="search" placeholder="Search...">
      <input type="number" name="from" class="difficulty" placeholder="Difficulty from..." min="1" max="6">
      <span>-</span>
      <input type="number" name="to" class="difficulty" placeholder="Difficulty to..." min="1" max="6">
      <input type="submit" value="search">
    </form>
    {{#each newCubes}}
    <div class="cube">
      <img class="cube" src="{{this.imageUrl}}">
      <p class="name">{{this.name}}</p>
      <p><span>Difficulty level:</span>{{this.difficultyLevel}}</p>
      <a class="btn" href="/details/{{this._id}}">Details</a>
    </div>
    {{else}}
    <h1>Error (no cubes in data base)</h1>
    {{/each}}
  </main>
</body>